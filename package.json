{
  "name": "thsr-app",
  "version": "1.1.0",
  "private": true,
  "homepage": "http://linyencheng.github.io/thsr-app",
  "dependencies": {
    "date-input-polyfill": "^2.14.0",
    "gh-pages": "^5.0.0",
    "humps": "^2.0.1",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.62.0",
    "sweetalert2": "^11.7.3",
    "swr": "^2.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "jest --coverage",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "make-badges": "coverage-badges",
    "eject": "react-scripts eject",
    "predeploy": "CI=false npm run build",
    "format": "prettier --write \"src/**/*.{js,scss}\"",
    "deploy": "gh-pages -b gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "coverage-badges-cli": "^1.1.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/containers/**/*.js",
      "src/util/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  }
}
