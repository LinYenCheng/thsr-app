{
  "name": "thsr-app",
  "version": "1.2.0",
  "private": true,
  "homepage": "http://linyencheng.github.io/thsr-app",
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "coverage-badges-cli": "^1.1.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "prettier": "^2.8.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.5.0",
    "date-input-polyfill": "^2.14.0",
    "gh-pages": "^5.0.0",
    "humps": "^2.0.1",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.0",
    "sweetalert2": "^11.10.2",
    "sweetalert2-react-content": "^5.0.7",
    "swr": "^2.2.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "coverage": "jest --coverage",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --watch=false",
    "make-badges": "coverage-badges",
    "predeploy": "CI=false npm run build",
    "format": "prettier --write \"src/**/*.{js,scss}\"",
    "deploy": "gh-pages -b gh-pages -d build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/containers/**/*.js",
      "src/util/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  }
}
